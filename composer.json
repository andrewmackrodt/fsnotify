{
  "name": "andrewmackrodt/fsnotify",
  "type": "library",
  "description": "Event based file system watcher for PHP",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Mackrodt",
      "email": "andrewmackrodt@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "amphp/process": "^0.3.3",
    "amphp/react-adapter": "dev-promise",
    "react/event-loop": "^0.5.3",
    "symfony/event-dispatcher": "^4.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12",
    "mockery/mockery": "^1.1",
    "phpunit/phpunit": "^7.2",
    "sempro/phpunit-pretty-print": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Denimsoft\\FsNotify\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Denimsoft\\FsNotify\\Test\\": "tests/"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "https://github.com/andrewmackrodt/react-adapter" }
  ],
  "config": {
    "sort-packages": true
  }
}
